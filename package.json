{
  "name": "n8n-nodes-base-blockchain",
  "version": "1.0.0",
  "description": "n8n community node for Base Blockchain",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/nodes/Base Blockchain/Base Blockchain.node.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "format": "prettier nodes credentials --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "base",
    "blockchain",
    "basescan",
    "ethereum"
  ],
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.55.0",
    "eslint-plugin-prettier": "^5.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "n8n": {
    "credentials": [
      "dist/credentials/BaseBlockchainApi.credentials.js"
